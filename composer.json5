{
  name: "statink/yii2-ip-badge",
  type: "yii2-extension",
  license: "MIT",
  description: "IP version badge for stat.ink header",
  "minimum-stability": "stable",
  require: {
    // env
    php: ">= 7.1.0",

    // packages
    "yiisoft/yii2": "~2.0.15",
  },
  config: {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  autoload: {
    "psr-4": {
      "statink\\yii2\\ipBadge\\": "src",
    },
  },
  authors: [
    {
      name: "AIZAWA Hina",
      email: "hina@bouhime.com",
    },
  ],
  repositories: [
    {
      type: "composer",
      url: "https://packagist.jp",
    },
    {
      type: "composer",
      url: "https://asset-packagist.org",
    },
  ],
}
